<rss version="2.0" xmlns:googleplay="http://www.google.com/schemas/play-podcasts/1.0/play-podcasts.xsd"
	xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<atom:link href="https://podcast-test.netlify.app/feed/feed.xml" rel="self" type="application/rss+xml" />
		<atom:link type="text/html" href="http://podlove.org/" />
		<title>Music stuff</title>
		<link>https://podcast-test.netlify.app/</link>
		<description>About Non-copyrighted music</description>

		<language>en-us</language>

		<googleplay:owner>v.yogapradeep@gmail.com</googleplay:owner>
		<googleplay:author>Pradeep</googleplay:author>

		<googleplay:image href="https://podcast-test.netlify.app/img/podcast.png" />
		<googleplay:category text="Music" />
		<googleplay:explicit>no</googleplay:explicit>


		<itunes:image href="https://podcast-test.netlify.app/img/podcast.png" />
		<itunes:explicit>no</itunes:explicit>
		<itunes:owner>
			<itunes:email>v.yogapradeep@gmail.com</itunes:email>
		</itunes:owner>
		<itunes:author>Pradeep</itunes:author>
		<itunes:category text="Music">
			<itunes:category text="Music History" />
		</itunes:category>
		<itunes:type>serial</itunes:type>


		{%- for post in collections.posts %}
		{% set absolutePostUrl %}{{ post.url | url | absoluteUrl(metadata.url) }}{% endset %}
		{%- if post.data.audio -%}
		<item>
			<guid isPermaLink="false">letmego</guid>
			<title>{{ post.data.epi_title }}</title>
			<description>{{post.data.epi_description}}</description>
			<pubDate>{{ post.data.pubdate | toUTC }}</pubDate>
			<link>https://podcast-test.netlify.app/posts/secondpost/index.html</link>
			<enclosure url="https://podcast-test.netlify.app/{{ post.data.audio }}" type="audio/mpeg"
				length="{{post.data.size}}" />
			<psc:chapters version="1.2" xmlns:psc="http://podlove.org/simple-chapters">
				<psc:chapter start="0" title="Start" />
				<psc:chapter start="1:35" title="halfway" href="" image="" />
				<psc:chapter start="2:00" title="Last Minute of song" />
			</psc:chapters>
			<googleplay:explicit>no</googleplay:explicit>
			<itunes:duration>{{post.data.duration}}</itunes:duration>
			<itunes:image href="https://podcast-test.netlify.app/img/podcast.png" />
			<itunes:keywords>{{post.data.keywords}}</itunes:keywords>
			<itunes:explicit>no</itunes:explicit>
			<itunes:episode>1</itunes:episode>
			<itunes:season>1</itunes:season>
		</item>
		{%- endif -%}
		{%- endfor %}
	</channel>
</rss>